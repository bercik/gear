<level>
    <meta>
        <author>Piotr Korzuszek</author>
        <email>piotr.korzuszek@gmail.com</email>
        <home>http://graniec.pl/trac/</home>
    
        <name>Demo Level</name>
        <version>1.0</version>
        
        <size>
            <width>10</width>
            <height>10</height>
        </size>
    </meta>
    
    <content>
        <!-- Define how track leads to -->
        <track>
            <block x="1" y="5" type="vert" start="yes" />
            <block x="1" y="4" type="vert" />
            <block x="1" y="3" type="vert" />
            <block x="1" y="2" type="vert" />
            <block x="1" y="1" type="turn_bottom_right" />
            <block x="2" y="1" type="horiz" />
            <block x="3" y="1" type="horiz" />
            <block x="4" y="1" type="turn_bottom_left" />
            <block x="4" y="2" type="vert" />
            <block x="4" y="3" type="vert" />
            <block x="4" y="4" type="turn_top_right" />
            <block x="5" y="4" type="horiz" />
            <block x="6" y="4" type="horiz" />
            <block x="7" y="4" type="horiz" />
            <block x="8" y="4" type="turn_bottom_left" />
            <block x="8" y="5" type="vert" />
            <block x="8" y="6" type="turn_top_left" />
            <block x="7" y="6" type="horiz" />
            <block x="6" y="6" type="horiz" />
            <block x="5" y="6" type="turn_bottom_right" />
            <block x="5" y="7" type="vert" />
            <block x="5" y="8" type="turn_top_left" />
            <block x="4" y="8" type="horiz" />
            <block x="3" y="8" type="horiz" />
            <block x="2" y="8" type="horiz" />
            <block x="1" y="8" type="turn_top_right" />
            <block x="1" y="7" type="vert" />
            <block x="1" y="6" type="vert" />
        </track>
        
        <bounds>
            <!--generate style="thick" /-->
            <bound from="2.0, 2.0" to="4.0, 2.0" />
            <bound from="4.0, 2.0" to="4.0, 5.0" />
            <bound from="4.0, 5.0" to="8.0, 5.0" />
        </bounds>
    </content>
</level>
